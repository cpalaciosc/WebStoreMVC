@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<div class="text-left">
    <h3 class="text-danger">Un error ha ocurrido con su petición. Consulte con el administrador.</h3>
    <p><span class="negrita">Error detail:</span> @Model.Exception</p>
</div>


